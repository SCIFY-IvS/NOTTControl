    <Method Name="M_Configure" Id="{49ee305e-ac0c-45ea-b7e2-293bc76ee32a}">
      <Declaration><![CDATA[METHOD M_Configure
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF NOT bConfigured	THEN
	bConfigured	:= TRUE;
	
	AstroData.mean.alpha	:= in_Ra_hhmmss;
	AstroData.mean.delta	:= in_Dec_ddmmss;
	
	// Set telescope coordinates
	CASE in_nTel OF 
		E_ASTRO_TELESCOPE.ELT:
			AstroData.site.longitude	:= F_astroDms2rad( 701119.20);
			AstroData.site.latitude		:= F_astroDms2rad(-243511.99);
			AstroData.site.height		:= 3046.0;
		
		E_ASTRO_TELESCOPE.UT1:
			AstroData.site.longitude	:= F_astroDms2rad( 702418.27);
			AstroData.site.latitude		:= F_astroDms2rad(-243739.44);
			AstroData.site.height		:= 2635.0;
		
		E_ASTRO_TELESCOPE.UT2:
			AstroData.site.longitude	:= F_astroDms2rad( 702417.39);
			AstroData.site.latitude		:= F_astroDms2rad(-243737.80);
			AstroData.site.height		:= 2635.0;
		
		E_ASTRO_TELESCOPE.UT3:
			AstroData.site.longitude	:= F_astroDms2rad( 702416.32);
			AstroData.site.latitude		:= F_astroDms2rad(-243736.64);
			AstroData.site.height		:= 2635.0;
		
		E_ASTRO_TELESCOPE.UT4:
			AstroData.site.longitude	:= F_astroDms2rad( 702414.24);
			AstroData.site.latitude		:= F_astroDms2rad(-243737.36);
			AstroData.site.height		:= 2635.0;
		
	END_CASE

END_IF]]></ST>
      </Implementation>
    </Method>

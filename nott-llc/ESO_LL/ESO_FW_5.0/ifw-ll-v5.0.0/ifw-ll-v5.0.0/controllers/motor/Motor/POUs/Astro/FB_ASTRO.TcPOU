<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.12">
  <POU Name="FB_ASTRO" Id="{1014d482-4860-4a3b-8dfd-8929b2722704}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_ASTRO
VAR_INPUT
	in_TimeStat:		REFERENCE TO T_TIME_STAT;	// REF to FB_TIMER.stat
	in_Ra_hhmmss:		LREAL	:= 0.0;
	in_Dec_ddmmss:		LREAL	:= -890000.0;
	in_nTel:			E_ASTRO_TELESCOPE	:= E_ASTRO_TELESCOPE.ELT;
END_VAR
VAR_OUTPUT
	// CCS Deterministic data that normally come 
	// from the Telescope via MUDPI interface.
	{attribute 'OPC.UA.DA' := '0'}
	out_CcsDeterministic:	T_ASTRO_POINTING_KERNEL_POSITIONS;
END_VAR
VAR

	// All data calculated in FB_ASTRO
	{attribute 'OPC.UA.DA' := '0'}
	AstroData: 				T_ASTRO_DATA;	
    UserPointingKernel: 	T_ASTRO_POINTING_KERNEL_POSITIONS;		
 
	
	{attribute 'OPC.UA.DA' := '0'}
	q_trk_data          AT %Q*: T_ASTRO_POINTING_KERNEL_POSITIONS;

	bConfigured:	BOOL;
	
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[// Copy input variables on start
M_Configure();

//
// Update all input parameters into AstroData structure.
//
M_AstroDataUpdate();

//
// Update tracking data, i.e. gmst, ra, dec, alt, az, pa, ha, etc.
// Update also CcsDeterministic output structure.
//
M_TrkUpdate();

// Set outputs for mapping.
q_trk_data	:= out_CcsDeterministic;
]]></ST>
    </Implementation>

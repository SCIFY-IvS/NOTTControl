    <Method Name="M_OffsetRaDec_arcsec" Id="{a29ca343-ab80-4f02-a702-12e551a63f85}">
      <Declaration><![CDATA[// Set Ra and Dec offsets in arcsec
METHOD M_OffsetRaDec_arcsec
VAR_INPUT
	in_lrOffsetRa_arcsec:	LREAL;	// Offset in Ra  [arcsec]
	in_lrOffsetDec_arcsec:	LREAL;	// Offset in Dec [arcsec]
END_VAR
VAR
	lrTmp:			LREAL;
	lrOffset_rad:	LREAL;	// Offset in [rad].
	lrOffset_h:		LREAL;	// Offset in [h].
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[//
// Ra offset
//

// Get Ra in rad
lrTmp	:= F_astroHms2rad(AstroData.mean.alpha);

// Convert arcsec->hour
lrOffset_h	:= in_lrOffsetRa_arcsec / 3600.0 * 24 / 360.0;

// Conversion h->rad
lrOffset_rad	:= lrOffset_h / 24.0 * 2*PI;

// Add the offset to the current Ra value in rad
lrTmp	:= lrTmp + lrOffset_rad;

// Convert rad to hhmmss.
AstroData.mean.alpha	:= F_astroRad2Hms(lrTmp);


//
// Dec offset
//

// Get Dec in rad
lrTmp	:= F_astroDms2rad(AstroData.mean.delta);

// Get the Dec offset in rad first.
// Conversion: arcsec->deg, and then deg->rad.
lrOffset_rad	:= (in_lrOffsetDec_arcsec / 3600.0) * PI / 180.0;

// Add the offset to the current Dec value in rad
lrTmp	:= lrTmp + lrOffset_rad;

// Convert rad to ddmmss.
AstroData.mean.delta	:= F_astroRad2Dms(lrTmp);

]]></ST>
      </Implementation>
    </Method>
    <Method Name="M_SetRaDec_hdmmss" Id="{0978c2fc-124c-4fa2-b6e2-1743a2b6da56}">
      <Declaration><![CDATA[// Set Ra in hhmmss.sss and Dec in ddmmss.sss
METHOD M_SetRaDec_hdmmss
VAR_INPUT
	in_lrRa:	LREAL;	// Ra  [hhmmss.sss]
	in_lrDec:	LREAL;	// Dec [ddmmss.sss]
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[AstroData.mean.alpha	:= in_lrRa;
AstroData.mean.delta	:= in_lrDec;
]]></ST>
      </Implementation>
    </Method>
    <Method Name="M_TrkUpdate" Id="{d5f63a1c-4c3c-4d6b-8408-002c5ac1d2b9}">
      <Declaration><![CDATA[METHOD M_TrkUpdate : BOOL
VAR_INPUT
END_VAR
VAR
	ra_r, dec_r:	LREAL;
	temp1, temp2, temp3, temp4:	LREAL;
	ut1, t, tu, A:	LREAL;
	
	sh, ch, cd, chcd, sp, sdcp, cp, sd, x, y, rsq:	LREAL;

	track:	REFERENCE TO T_ASTRO_TRACKING_DATA;
	coord:	REFERENCE TO T_ASTRO_SITE_CONSTANTS;
	time_data:	REFERENCE TO T_ASTRO_TIME;
	
	latitude, longitude:	LREAL;
	
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[// This is the slalib C code translated into ST.
//
// HRESULT TrkUpdate()
//

//
// Set references to output data structures
//
track		REF=AstroData.tracking_data;
coord		REF=AstroData.site;
time_data	REF=AstroData.time_data;


// Copy ra and dec from input to output data structures
track.ra_hms  := AstroData.mean.alpha;
track.dec_dms := AstroData.mean.delta;

// Get ra and dec in radians and degrees
track.ra_r		:= F_astroHms2rad(track.ra_hms);
track.ra_deg	:= track.ra_r * GCL.C_RAD2DEG;
track.dec_r		:= F_astroDms2rad(track.dec_dms);
track.dec_deg	:= track.dec_r * GCL.C_RAD2DEG;

ra_r	:= track.ra_r;
dec_r	:= track.dec_r;
	

latitude	:= AstroData.site.latitude;
longitude	:= AstroData.site.longitude;

temp1 := COS(dec_r) * TAN(latitude);
temp2 := SIN(dec_r);
temp3 := temp2 * SIN(latitude);
temp4 := COS(dec_r) * COS(latitude);


//
// Calculate times
//
ut1 := F_timeUTCToMJD(F_timeGetUTC(in_TimeStat.lUtcTime));	
tu := (ut1 - 51544.5) / 36525.0;

// Updata T_ASTRO_TIME structure
time_data.leap_second	:= in_TimeStat.el6688.diagnostics.leap_second;
IF time_data.leap_second = 0	THEN
	time_data.leap_second	:= 37;
END_IF
time_data.lUtcTime		:= in_TimeStat.lUtcTime;
time_data.sUtcTime		:= in_TimeStat.sUtcTime;
time_data.ut1			:= ut1;
time_data.tu			:= tu;

time_data.gmst :=	F_vltMOD(ut1, 1.0) * GCL.C_2PI +
				(24110.54841 + (8640184.812866 + (0.093104 - 6.2E-6 * tu) * tu) * tu) * GCL.C_S2R;
time_data.gmst := F_vltMOD(time_data.gmst, GCL.C_2PI);

IF time_data.gmst < 0.0 THEN
	time_data.gmst := time_data.gmst + GCL.C_2PI;
END_IF

//
// Hour angle
//
track.ha_r := time_data.gmst - (longitude + ra_r);	// TODO: Check the formula

// Hour angle is in range 0..2PI [rad], i.e. 0..24 [hour]
IF track.ha_r > GCL.C_2PI THEN
	track.ha_r	:= track.ha_r - GCL.C_2PI;
ELSIF track.ha_r < 0.0 THEN
	track.ha_r	:= track.ha_r + GCL.C_2PI;
END_IF

track.ha_deg	:= track.ha_r * GCL.C_RAD2DEG;
track.ha_hms	:= F_astroRad2Hms(track.ha_r);

//
// CALC. parallactic angle 
//
// t = tan(latitude) * cos(dec) - sin(dec) * cos(hourAngle); 
t := temp1 - temp2 * COS(track.ha_r);
IF t = 0.0 THEN
	M_TrkUpdate := FALSE;  // error 
	RETURN;
END_IF


// 
// Parallactic angle
// 
track.pa_r := F_slaPa(track.ha_r, dec_r, latitude);

// Original PA calculation
track.pa_r := ATAN(SIN(track.ha_r) / t) + GCL.C_PI;
track.pa_r := F_vltMod(GCL.C_PI2 * F_vltSign(t) + track.pa_r + GCL.C_PI2 , GCL.C_2PI) +
					GCL.C_PI2 * (-F_vltSign(track.ha_r) + 1) * 
				  (F_vltSign(dec_r - latitude) - 1);	
// Keep -PI < PA < PI
IF track.pa_r > GCL.C_PI THEN
	track.pa_r := track.pa_r - GCL.C_2PI;
ELSIF track.pa_r < -GCL.C_PI THEN
	track.pa_r := track.pa_r + GCL.C_2PI;
END_IF

track.pa_deg := track.pa_r * GCL.C_RAD2DEG;


//
// CALC. telescope altitude in radians and degrees.
//
// altitude = asin(sin(dec) * sin(latitude) + cos(dec) * cos(latitude) * cos(hourAngle));
track.alt_r := ASIN(temp3 + temp4 * COS(track.ha_r));
track.alt_deg := track.alt_r * GCL.C_RAD2DEG;

//
// DPO: slaE2h()
//
(*
	* A = acos((sin(dec) - sin(altitude) * sin(latitude))
	*             ---------------------------------------
	*			      cos(altitude)*cos(latitude))
*)
sh := SIN(track.ha_r);
ch := COS(track.ha_r);
sd := SIN(dec_r);
cd := COS(dec_r);
sp := SIN(latitude);
cp := COS(latitude);
chcd := ch * cd;
sdcp := sd * cp;
x := - chcd * sp + sdcp;
y := - sh * cd;
rsq := x *x + y * y;

IF rsq <> 0.0 THEN
	A := F_atan2(y,x);
ELSE
	A := 0.0;
END_IF


// The above formula assumes 0deg North and 90deg East. This
// must be corrected for VLT conventions 0deg is South. The 
// complete range of azimuth will be between 0-360.
// if (A < 0.0) A += vlt2PI;
A := - (A + GCL.C_PI);
IF A < 0.0 THEN
	A := A + GCL.C_2PI;
END_IF
A := F_vltMOD(A,GCL.C_2PI);

track.az_r 		:= A;
track.az_deg	:= track.az_r * GCL.C_RAD2DEG;

//
// CALC. Field Rotation 
//
track.fr_r		:= track.pa_r -  track.alt_r;
track.fr_deg	:= track.fr_r * GCL.C_RAD2DEG;

//////////////////////////
// Populate OUT structure
//////////////////////////

out_CcsDeterministic.current_observed_altaz[0]	:= track.alt_r;
out_CcsDeterministic.current_observed_altaz[1]	:= track.az_r;

out_CcsDeterministic.target_observed_altaz[0]	:= track.alt_r;
out_CcsDeterministic.target_observed_altaz[1]	:= track.az_r;

out_CcsDeterministic.time_tai	:= in_TimeStat.lrTaiUnixTimeinSecs;
out_CcsDeterministic.time_utc	:= in_TimeStat.lrTaiUnixTimeinSecs + time_data.leap_second;

out_CcsDeterministic.radec_at_altaz_at_requested_xy[0]	:= track.ra_r;
out_CcsDeterministic.radec_at_altaz_at_requested_xy[1]	:= track.dec_r;

out_CcsDeterministic.observed_altaz_at_requested_xy[0]	:= 0.0;
out_CcsDeterministic.observed_altaz_at_requested_xy[1]	:= 0.0;

out_CcsDeterministic.time_lst := track.ha_r + track.ra_r;

out_CcsDeterministic.parallactic_angle	:= track.pa_r;
out_CcsDeterministic.north_angle	:= track.pa_r + track.alt_r;
out_CcsDeterministic.pupil_angle	:= track.alt_r;



UserPointingKernel.parallactic_angle 	:= track.pa_r * GCL.C_RAD2DEG;
UserPointingKernel.north_angle 			:= out_CcsDeterministic.north_angle * GCL.C_RAD2DEG;
UserPointingKernel.pupil_angle 			:= track.alt_r * GCL.C_RAD2DEG;
UserPointingKernel.radec_at_altaz_at_requested_xy[0]:= F_astroRad2Hms(track.ra_r);
UserPointingKernel.radec_at_altaz_at_requested_xy[1]:= F_astroRad2Dms(track.dec_r);
UserPointingKernel.target_observed_altaz[0]		:= track.alt_r  * GCL.C_RAD2DEG;
UserPointingKernel.target_observed_altaz[1]		:= track.az_r  * GCL.C_RAD2DEG;
UserPointingKernel.current_observed_altaz[0]	:= track.alt_r  * GCL.C_RAD2DEG;
UserPointingKernel.current_observed_altaz[1]	:= track.az_r  * GCL.C_RAD2DEG;

M_TrkUpdate := TRUE;  // OK 

]]></ST>
      </Implementation>
    </Method>

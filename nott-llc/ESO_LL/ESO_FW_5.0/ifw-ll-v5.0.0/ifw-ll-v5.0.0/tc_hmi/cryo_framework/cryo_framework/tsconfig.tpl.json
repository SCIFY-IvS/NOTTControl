{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compileOnSave": true,
  "compilerOptions": {
    "module": "none",
    "target": "es6",
    "skipLibCheck": true,
    "lib": [
      "dom",
      "scripthost",
      "es6",
      "es2017.intl"
    ],
    "types": [],
    "declaration": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictBindCallApply": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "alwaysStrict": false
  },
    "include": [
        "$(Beckhoff.TwinCAT.HMI.Framework).InstallPath/TcHmi.d.ts",
        "$(Beckhoff.TwinCAT.HMI.Controls).InstallPath/TcHmiRectangle/TcHmiRectangle.d.ts",
        "$(Beckhoff.TwinCAT.HMI.Controls).InstallPath/TcHmiTextblock/TcHmiTextblock.d.ts",
        "$(Beckhoff.TwinCAT.HMI.Framework).InstallPath/Controls/System/TcHmiContainer/TcHmiContainer.d.ts"
    ],
    "files": [
        "Controls/pump/pump.ts",
        "Controls/signalbox/signalbox.ts",
        "Controls/led/led.ts",
        "Controls/valve/valve.ts"
    ]
}

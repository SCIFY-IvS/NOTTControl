  <POU Name="MAIN" Id="{0e8b7f5d-63c1-4068-8341-41a067b6b414}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM MAIN
VAR
	fbTimer: FB_TIME;
	fbReceiver: FB_CCS_RECEIVER;
	{attribute 'OPC.UA.DA':='1'}
	drot:			FB_MA_DROT;
	{attribute 'OPC.UA.DA':='1'}
	sim_drot:	FB_SIM_MOTOR;
	
	
	
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[fbTimer();
fbReceiver(refTimeObj := fbTimer);
//
//Devices
//

	 in_lrRotation :=fbReceiver.tracking_ccs.north_angle,
	 in_lrPupil := fbReceiver.tracking_ccs.pupil_angle,
	 in_lrParallactic :=fbReceiver.tracking_ccs.pa,
	 nNOVRAM_DevId:=0);
	 
sim_drot(ptrCfg:=ADR(drot.motor.cfg), ptrStat:=ADR(drot.motor.stat));	 ]]></ST>
